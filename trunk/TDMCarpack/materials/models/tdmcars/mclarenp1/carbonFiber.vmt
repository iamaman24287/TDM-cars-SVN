"VertexlitGeneric"
{
	"$basetexturetransform" "center .5 .5 scale 3 3 rotate 0 translate 0 0"
	"$basetexture" "models/tdmcars/shared/carbonFiber"
	"$bumpmap" "models/tdmcars/shared/carbonFiberNormal"
	"$nodecal" 1
	"$phong" 1
	"$phongexponent" 20
	"$phongfresnelranges"	"[1 1 1]"
	"$phongboost"	0.9
	$envmap env_cubemap
	$envmaptint "[1 1 1]"
	"$colorfix" "{255 255 255}"
	"Proxies"
	{
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}
	}
}
